<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="fenetre" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Vue.ControllerEntite">
   <children>
      <GridPane prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" />
          <ColumnConstraints hgrow="SOMETIMES" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints vgrow="SOMETIMES" />
          <RowConstraints vgrow="SOMETIMES" />
          <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nom : " GridPane.halignment="RIGHT">
               <font>
                  <Font name="Microsoft JhengHei Light" size="14.0" />
               </font></Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Image : " GridPane.halignment="RIGHT" GridPane.rowIndex="1">
               <font>
                  <Font name="Microsoft JhengHei Light" size="14.0" />
               </font></Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Type : " GridPane.halignment="RIGHT" GridPane.rowIndex="2">
               <font>
                  <Font name="Microsoft JhengHei Light" size="14.0" />
               </font></Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Categorie : " GridPane.halignment="RIGHT" GridPane.rowIndex="3">
               <font>
                  <Font name="Microsoft JhengHei Light" size="14.0" />
               </font></Text>
            <Label fx:id="name" GridPane.columnIndex="1" />
            <Button fx:id="path" mnemonicParsing="false" onAction="#loadImage" text="Chercher" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font name="Microsoft JhengHei Light" size="13.0" />
               </font></Button>
            <ComboBox fx:id="types" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
            <ComboBox fx:id="categories" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Button fx:id="buttonOK" mnemonicParsing="false" onAction="#handleOk" text="OK" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="CENTER">
               <font>
                  <Font name="Microsoft JhengHei Light" size="14.0" />
               </font></Button>
            <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="CENTER">
               <font>
                  <Font name="Microsoft JhengHei Light" size="14.0" />
               </font></Button>
         </children>
      </GridPane>
   </children>
</AnchorPane>
